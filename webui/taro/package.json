{
  "name": "agi-view",
  "version": "0.0.1",
  "private": true,
  "description": "没谱儿的AGI运营的AGI观点栏目",
  "templateInfo": {
    "name": "vue3-pinia",
    "typescript": true,
    "css": "Sass",
    "framework": "Vue3"
  },
  "scripts": {
    "build:worker": "tsc -p tsconfig.worker.json",
    "build:weapp": "rm -rf dist/weapp/worker && yarn build:worker && taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "prettier:check": "prettier --check \"**/*.{js,json,md,ts,yml}\"",
    "prettier:write": "prettier --write \"**/*.{js,json,md,ts,yml}\""
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ],
  "author": "kikakkz@hotmail.com",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "4.0.12",
    "@tarojs/helper": "4.0.12",
    "@tarojs/plugin-framework-vue3": "4.0.12",
    "@tarojs/plugin-platform-alipay": "4.0.12",
    "@tarojs/plugin-platform-h5": "4.0.12",
    "@tarojs/plugin-platform-jd": "4.0.12",
    "@tarojs/plugin-platform-qq": "4.0.12",
    "@tarojs/plugin-platform-swan": "4.0.12",
    "@tarojs/plugin-platform-tt": "4.0.12",
    "@tarojs/plugin-platform-weapp": "4.0.12",
    "@tarojs/runtime": "4.0.12",
    "@tarojs/shared": "4.0.12",
    "@tarojs/taro": "4.0.12",
    "crypto-js": "^4.2.0",
    "pinia": "^2.0.10",
    "prettier": "^3.5.3",
    "taro-axios": "^1.1.1",
    "vue": "^3.2.24"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@tarojs/cli": "4.0.12",
    "@tarojs/taro-loader": "4.0.12",
    "@tarojs/webpack5-runner": "4.0.12",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-preset-taro": "4.0.12",
    "css-loader": "3.4.2",
    "eslint": "^8.12.0",
    "eslint-config-taro": "4.0.12",
    "eslint-plugin-vue": "^8.0.0",
    "postcss": "^8.4.18",
    "style-loader": "1.3.0",
    "stylelint": "^14.4.0",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^5.1.0",
    "vue-loader": "^17.0.0",
    "webpack": "5.91.0"
  }
}
